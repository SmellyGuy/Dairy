Document doc=Jsoup.parse(html);

Elements ele=doc.getElementsByTag("imag");
String imagSrc=ele.attr("src");
StringBuffer buff=new StringBuffer("The resorces:");
if(!"".equals(imagSrc)&&imagSrc.startWith("http://")){
    buff.append(imagSrc);
}
